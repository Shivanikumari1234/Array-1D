//write a program to add the elements of array
import java.util.Scanner;
public class A48 {
    public static void main(String[] args)
    {
        int a[]=new int[5];
        int sum=0;
        Scanner r=new Scanner(System.in);
        System.out.print("Enter the Elements in array");
        for(int i=0;i<5;i++)
        {
            a[i]=r.nextInt();
            sum=a[i]+sum;
        }
        System.out.print("\nAddition of the array elements" + sum);

    }
}
